{
    "inbounds": [
        {
            "type": "trojan",
            "tag": "trojan-in",
            "listen": "0.0.0.0",
            "listen_port": 443,
            "users": [
                {
                    "name": "admin",
                    "password": "123456"
                }
            ],
            "tls": {
                "enabled": true,
                "certificate_path": "/ssl_certs/cert.pem",
                "key_path": "/ssl_certs/key.pem",
                "alpn": [
                    "h2",
                    "http/1.1"
                ]
            },
            "fallback": {
                "server": "service-nginx",
                "server_port": 80
            },
            "multiplex": {
                "enabled": true
            },
            "transport": {}
        }
    ],
    "outbounds": [
        {
            "type": "direct"
        }
    ]
}